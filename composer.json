{
    "name": "manus/recharge-store",
    "description": "MVP Recharge Store",
    "require": {
        "flightphp/core": "^3.17",
        "symfony/dotenv": "^7.4"
    },
    "authors": [
        {
            "name": "Manus",
            "email": "manus@example.com"
        }
    ],
    "require-dev": {
        "symfony/var-dumper": "^7.4"
    },
    "scripts": {
        "post-autoload-dump": [
            "php -r \"if (!file_exists('.env')) copy('.env.example', '.env');\""
        ],
        "serve": "php -S localhost:61001 -t public"
    }
}
