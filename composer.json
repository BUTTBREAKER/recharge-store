{
    "require": {
        "flightphp/container": "^1.3",
        "flightphp/core": "^3.17",
        "illuminate/support": "^12.44",
        "leafs/auth": "^4.2",
        "leafs/db": "^4.0",
        "leafs/form": "^3.2",
        "leafs/password": "^1.0",
        "leafs/session": "^4.0",
        "symfony/dotenv": "^7.4",
        "symfony/string": "^7.4",
        "symfony/translation-contracts": "^3.6"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^4.0",
        "symfony/var-dumper": "^7.4"
    },
    "scripts": {
        "post-autoload-dump": [
            "php -r \"if (!file_exists('.env')) copy('.env.example', '.env');\""
        ],
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "php -S localhost:61001 -t public"
        ],
        "format": "phpcbf",
        "lint": "phpcs"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "config": {
        "sort-packages": true
    }
}
